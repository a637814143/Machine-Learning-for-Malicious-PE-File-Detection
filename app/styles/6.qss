QDialog {
    background-color: #ffffff;
    color: #000000;
    font-family: "Roboto", sans-serif;
    font-size: 14px;
}

QPushButton {
    background-color: #f5f5f5;
    color: #000000;
    border: 1px solid #6200ee;
    border-radius: 4px;
    padding: 6px 12px;
}

QPushButton:hover {
    background-color: #e0e0e0;
}

QLineEdit {
    background-color: #ffffff;
    color: #000000;
    border: 1px solid #6200ee;
    padding: 4px;
}

QLabel {
    color: #000000;
}